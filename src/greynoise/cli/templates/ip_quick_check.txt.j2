{% for ip_quick_check in results %}
{%- if ip_quick_check.noise and ip_quick_check.riot%}
<noise>{{ ip_quick_check.ip }}</noise> is classified as <bold>NOISE</bold> and is part of <bold>RIOT</bold>.
{%- elif ip_quick_check.noise %}
<noise>{{ ip_quick_check.ip }}</noise> is classified as <bold>NOISE</bold>.
{%- elif ip_quick_check.riot %}
<riot>{{ ip_quick_check.ip }}</riot> is part of <bold>RIOT</bold>.
{%- elif ip_quick_check.message %}
{{ ip_quick_check.message }}
{%- else %}
<not-noise>{{ ip_quick_check.ip }}</not-noise> is classified as <bold>NOT NOISE</bold>.
{%- endif %}
{%- endfor %}
