{% import "macros.txt.j2" as macros %}
{% for ip_context in results -%}
{% call macros.header(loop) %}Community{% endcall -%}

{%- if ip_context.noise or ip_context.riot %}
<key>IP</key>: <value>{{ ip_context.ip }}</value>
<key>NOISE</key>: <value>{{ ip_context.noise }}</value>
<key>RIOT</key>: <value>{{ ip_context.riot }}</value>
<key>Name</key>: <value>{{ ip_context.name }}</value>
<key>Classification</key>: {{ macros.classification(ip_context.classification) }}
<key>Last seen</key>: <value>{{ ip_context.last_seen }}</value>
<key>Link</key>: <value>{{ ip_context.link }}</value>
{% else %}
{{ ip_context.ip }} is not found in GreyNoise datasets.
{% endif %}
{% endfor %}
